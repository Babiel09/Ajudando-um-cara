generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Bingo{
   id       String    @id @default(auto()) @map("_id") @db.ObjectId
  nome String
}

model Users{
   id       String    @id @default(auto()) @map("_id") @db.ObjectId
  wins Int
  coins Float
  email String
  pass String
  role Usereses
}

model Itens{
  id       String    @id @default(auto()) @map("_id") @db.ObjectId
  nome String
  price Float
}


enum Usereses {
  Follow
  Sub
  Vip
}
